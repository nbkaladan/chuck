{
  "extends": "eslint:recommended",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": false
    },
    "allowImportExportEverywhere": true
  },
  "plugins": ["jasmine"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jasmine": true
  }
}
